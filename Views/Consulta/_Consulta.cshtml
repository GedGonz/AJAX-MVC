@model IEnumerable<MVCAJAXA.Models.PersonaModel>

<table>
    <thead>
    <tr>
        <td>Codigo</td>
        <td>Nombre</td>
        <td>Apellido</td>
    </tr>

    </thead>
    <tbody>
        @{foreach(var item in Model )
          {
            <tr>
                <td>@item.Codigo</td>
                <td>@item.Nombre</td>
                <td>@item.Apellido</td>
            </tr>

          }
        }
    </tbody>
</table>